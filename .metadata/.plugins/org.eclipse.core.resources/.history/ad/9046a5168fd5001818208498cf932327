	private byte[] compiledObject;
	private ArrayList<Reloc> compiledRelocs;
	
	public ASMObject() {
		this.chunks = new ArrayList<ObjectChunk>();
	}
	
	public void addChunk(ObjectChunk o) {
		chunks.add(o);
	}
	
	public void compile() {
		
	}
	
	
	
}
