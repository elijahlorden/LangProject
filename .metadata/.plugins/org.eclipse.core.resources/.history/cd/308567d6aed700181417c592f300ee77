package main;

public class MachineInfo {
	
	public static final String[] registerAliases = {
		"r0", //r0 - r7 (Registers 0 through 7) are general-purpose registers (nonvolatile)
		"r1",
		"r2",
		"r3",
		"r4",
		"r5",
		"r6",
		"r7",
		"ip", //ip (Register 8) is the instruction pointer
		"sp" //sp (Register 9) is the stack pointer
	};
	
}
